{
    "name": "awuniversity/oauth2-aw",
    "description": "OAuth 2.0 Client AW",
    "license": "MIT",
    "keywords": [
    ],
    "authors": [
        {
            "name": "Jurek"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "awuniversity/oauth2-client": "*"
    },
    "require-dev": {
        "eloquent/phony-phpunit": "^2.0",
        "phpunit/phpunit": "^6.0",
        "php-coveralls/php-coveralls": "^2.1",
        "squizlabs/php_codesniffer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "AwU\\OAuth2\\Client\\": "src/"
        }
